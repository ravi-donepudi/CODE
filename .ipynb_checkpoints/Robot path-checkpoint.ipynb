    "def power_set(lst):\n",
    "    if lst==[]:\n",
    "        return [[]]\n",
    "    else:\n",
    "        a= lst.pop(0)\n",
    "        subsets_without_a = power_set(lst)\n",
    "        subsets_with_a = [[a]+s for s in subsets_without_a]\n",
    "        return subsets_without_a+subsets_with_a\n",
    "    \n",
    "power_set([1,2,3,4])"
   ]
